<application>
  <component name="WebServers">
    <sftpAdvancedGlobalOptions>
      <SftpAdvancedOptions hostKeyCheckingLevel="NO">
        <option name="hostKeyCheckingLevel" value="NO" />
      </SftpAdvancedOptions>
    </sftpAdvancedGlobalOptions>
    <option name="servers">
      <webServer id="ssh://nsantos@localhost:8022/home/nsantos/.virtualenvs/bin/python" name="ssh://nsantos@localhost:8022" url="http://127.0.0.1">
        <fileTransfer host="127.0.0.1" port="8022" privateKey="$USER_HOME$/.ssh/id_rsa" accessType="SFTP" keyPair="true">
          <option name="port" value="8022" />
        </fileTransfer>
      </webServer>
      <webServer id="eab3a976-003f-47f9-9b19-0170063f8c6f" name="RaspberryPi" url="http://raspberrypi.local">
        <fileTransfer host="raspberrypi.local" port="22" accessType="SFTP">
          <advancedOptions>
            <advancedOptions dataProtectionLevel="Private" />
          </advancedOptions>
          <option name="port" value="22" />
        </fileTransfer>
      </webServer>
    </option>
  </component>
</application>